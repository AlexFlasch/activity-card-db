{
  "env": {
    "mocha": true
  },
  "globals": {
    "assert": true,
    "expect": true,
    "should": true,
    "sinon": true,
    "__static": true
  },
  "rules": {
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "no-new": 0,
    "space-before-function-paren": 0,
    "no-unused-expressions": 0,
  }
}
